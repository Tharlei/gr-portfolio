{
  "name": "giovanna-ramaldo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check '*.{ts,js,scss}'",
    "check-lint": "eslint . --ext ts --ext tsx --ext js",
